{
    "name": "rsbuild-auto-i18n-plugin",
    "version": "1.1.11",
    "description": "Rsbuild automatic translation plugin compatible with Rspack",
    "keywords": [
        "i18n",
        "auto i18n",
        "rsbuild i18n",
        "rspack i18n",
        "vue i18n",
        "react i18n"
    ],
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
    },
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README*"
    ],
    "scripts": {
        "build": "rollup -c ./rollup.config.ts --configPlugin typescript"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:wenps/auto-i18n-translation-plugins.git"
    },
    "author": "callqh",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/wenps/auto-i18n-translation-plugins/issues"
    },
    "homepage": "https://github.com/wenps/auto-i18n-translation-plugins#readme",
    "dependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-typescript": "^7.23.2",
        "auto-i18n-plugin-core": "workspace:*"
    },
    "devDependencies": {
        "@babel/types": "^7.23.0",
        "@rollup/plugin-babel": "^6.0.4",
        "rollup-plugin-terser": "^7.0.2",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/babel__core": "^7.20.3",
        "@types/node": "^20.3.3",
        "babel-plugin-import": "^1.13.6",
        "rollup": "^4.3.0",
        "rollup-plugin-dts": "^6.1.0",
        "rollup-plugin-node-externals": "^7.1.2",
        "typescript": "^5.1.6",
        "@rsbuild/core": "^1.0.0"
    },
    "peerDependencies": {
        "@rsbuild/core": ">=1.0.0"
    },
    "volta": {
        "extends": "../../package.json"
    }
}